<div class="container">
  <h1 class="center">Login</h1>
  <hr/>

  <div class="row">
    <div class="col-xs-12 col-md-8 col-md-offset-2 well">
      <%= form_for(:session, url: login_path) do |f| %>
        <%= f.label :email %>
        <%= f.email_field :email %>
        <%= f.label :password %>
        <%= f.password_field :password %>
  
        <%= f.submit "Login", class: "btn btn-success" %>
      <% end %>
      </br>
      <p>New user? <%= link_to "Register now!", register_path %></p>
    </div>
  </div>


</div>